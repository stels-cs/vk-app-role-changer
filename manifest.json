{
  "manifest_version": 2,
  "name": "Change role in app",
  "version": "0.0.1",
  "author": "Ivan Nedzvetskiy",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16.png",
      "24": "img/24.png",
      "32": "img/32.png"
    },
    "default_title": "Change role",
    "default_popup": "popup/popup.html"
  },
  "permissions":[
    "storage",
    "tabs",
    "https://*.vk.com/*",
    "activeTab"
  ]
}